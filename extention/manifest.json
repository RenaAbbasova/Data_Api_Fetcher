{
    "manifest_version": 3,
    "name": "API Data Fetcher",
    "version": "1.0",
    "description": "Fetches and displays data from multiple APIs",
    "permissions": [
      "alarms",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  
  
  
  